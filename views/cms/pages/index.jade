

extends ../../layout

block config
  - var body_class = 'index';

block content
  != outputContentBlock('slideshow', page)
  ul#sounds
    li.solos-and-duos
      a(href="/sounds/solos-and-duos")
        img(src="/images/layout/solos-and-duos-sm.jpg", alt="Solo and Duo Sounds")
    li.trios-or-larger
      a(href="/sounds/trios-or-larger")
        img(src="/images/layout/trios-or-larger-sm.jpg", alt="Trios or Larger")
    li.wedding-and-acoustic
      a(href="/sounds/wedding-and-acoustic")
        img(src="/images/layout/wedding-and-acoustic-sm.jpg", alt="Wedding and Acoustic")
  #content-main-outer
    != outputContentBlock('main', page)
